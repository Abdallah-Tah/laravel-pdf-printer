{
    "name": "amohamed/pdf-printer",
    "description": "A Laravel package to print PDF files directly to the default printer.",
    "require": {
        "php": ">=7.4",
        "mike42/escpos-php": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Amohamed\\PdfPrinter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abdallah Mohamed",
            "email": "abdal_cascad@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Amohamed\\PdfPrinter\\PdfPrinterServiceProvider"
            ]
        }
    }
}